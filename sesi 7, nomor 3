#include <iostream>
using namespace std;


int main() {
  int x,jumlah,min,max;
  int nilai[20] =  {23, 6, 89,12, 45, 67, 9, 90,54, 30};

 for (int i = 0; i < 10; i++) {
        x = nilai[i];
   if ( i == 1 ) {
    min = x;
    max = x;
    }
    else if ( min > x ) {
    min = x;
    }
    else if ( max < x) {
     max = x;
    }
     else {}
     }
   cout << endl;
       cout << "Nilai minimal nya adalah = " <<  min << endl;
       cout << "Nilai maximal nya adalah = " <<  max << endl;
  
  }
